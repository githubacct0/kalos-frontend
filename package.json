{
  "name": "@kalos-core/kalos-frontend",
  "version": "0.0.4",
  "description": "Kalos frontend components rewritten in React",
  "scripts": {
    "publish-pkg": "npm login && yarn publish --access restricted",
    "build-index": "gulp -LL index",
    "start": "gulp -LL start",
    "update-api": "yarn add @kalos-core/kalos-rpc@latest && gulp -LL copy",
    "clean": "gulp -LL clean",
    "bundle": "gulp -LL bundle",
    "make": "gulp -LL create",
    "release": "gulp -LL release",
    "release-all": "gulp -LL release-all",
    "distribute": "gulp -LL distribute",
    "bust": "gulp -LL bust",
    "less": "lessc ./styles/index.less ./styles/main.css",
    "upload": "gulp -LL upload",
    "dev-next": "cd App && next dev",
    "build-next": "cd App && next build",
    "start-next": "cd App && next start",
    "lint-next": "cd App && next lint",
    "test-coverage": "nyc ts-mocha --recursive -r ./test/test-setup/setup.ts -r esm -r ignore-styles -r mock-local-storage --check-leaks",
    "test": "ts-mocha --recursive -r ./test/test-setup/setup.ts -r esm -r ignore-styles -r mock-local-storage --reporter mochawesome --check-leaks",
    "test-watch": "ts-mocha --grep RPC --invert --recursive -r ./test/test-setup/setup.ts -r esm -r ignore-styles  -r mock-local-storage -w --reporter mochawesome --check-leaks",
    "test-watch-parallel": "ts-mocha --parallel --grep RPC --invert --recursive -r ./test/test-setup/setup.ts -r esm -r ignore-styles  -r mock-local-storage -w --reporter mochawesome --check-leaks"
  },
  "author": "Robert Milejczak <robbie@kalosflorida.com>",
  "license": "MIT",
